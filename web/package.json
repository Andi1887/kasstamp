    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "openapi-typescript": "^7.4.3",
    "ts-jest": "^29.1.0",
