    "kaspa",
    "kas",
    "blockchain",
    "blockdag",
    "proof-of-existence",
    "pow"
  ],
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run clean && turbo run build",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint:packages": "npm run lint --workspaces --if-present",
    "format": "npx prettier --write \"packages/**/*.{ts,js,json,md}\" \"*.{ts,js,json,md}\"",
    "format:check": "npx prettier --check \"packages/**/*.{ts,js,json,md}\" \"*.{ts,js,json,md}\"",
    "type-check": "tsc --build --noEmit tsconfig.build.json",
    "type-check:packages": "npm run type-check --workspaces --if-present",
    "validate": "npm run type-check && npm run lint && npm run test",
    "validate:packages": "npm run validate --workspaces --if-present",
    "clean": "rimraf packages/*/dist packages/*/tsconfig.tsbuildinfo",
    "clean:all": "rimraf node_modules packages/*/node_modules packages/*/dist packages/*/tsconfig.tsbuildinfo",
    "dev": "npm run build:tsc -- --watch",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:all": "npm run audit --workspaces --if-present && npm audit",
    "outdated": "npm outdated",
    "outdated:all": "npm run outdated --workspaces --if-present && npm outdated"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "babel-jest": "^30.2.0",
    "commander": "^14.0.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.38.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.2.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.2",
    "tsup": "^8.0.0",
    "tsx": "^4.20.5",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "blakejs": "^1.2.1",
    "jest-environment-jsdom": "^30.2.0",
    "openapi-fetch": "^0.15.0",
    "uuid": "^13.0.0"
  }
}
{
  "name": "@kasstamp/kaspa_api",
  "version": "0.0.1",
  "description": "Utilities for Kasstamp",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "license": "ISC",
  "scripts": {
    "generate": "openapi-typescript https://api.kaspa.org/openapi.json -o src/generated/kaspa-api.ts && npx prettier --write src/generated/**/*.ts",
    "generate:testnet": "openapi-typescript https://api-testnet-10.kaspa.org/openapi.json -o src/generated/kaspa-api-testnet.ts && npx prettier --write src/generated/**/*.ts",
    "build": "npm run build:bundle",
    "build:bundle": "tsup --dts src/index.ts --format esm --splitting false --clean --no-splitting --sourcemap"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
